<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.speechrecificationapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpeechRecificationApp">
        <activity android:name=".Exercise"></activity>
        <activity android:name=".therapist_list" />
        <activity android:name=".About" />
        <activity android:name=".Speech_Therapist_Activity" />
        <activity android:name=".view.DetailsActivity" />
        <activity android:name=".view.ItemsActivity" />
        <activity android:name=".view.UploadActivity" />
        <activity android:name=".UserlistActivity" />
        <activity android:name=".DatasetActivity" />
        <activity android:name=".Text_to_Speech" />
        <activity android:name=".Dashboard" />
        <activity android:name=".Sound_Practice" />
        <activity android:name=".second_screen" />
        <activity android:name=".Signup_Activity" />
        <activity android:name=".Front_Animation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Speech_Activity" />
        <activity android:name=".LoginActivity" />
    </application>

</manifest>